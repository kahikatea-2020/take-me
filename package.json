{
  "name": "boilerplate-react-webpack",
  "version": "0.2.2",
  "description": "A webpack React starter",
  "repository": "https://github.com/dev-academy-challenges/boilerplate-react-webpack.git",
  "main": "server/index.js",
  "author": "EDA",
  "license": "ISC",
  "scripts": {
    "client": "webpack-dev-server --hot --inline",
    "dev": "run-p dev:client dev:server",
    "dev:client": "webpack --watch",
    "dev:server": "nodemon server",
    "start": "run-s build server",
    "build": "webpack --config webpack.config.js",
    "server": "node server",
    "test": "jest --watchAll",
    "test:watch": "npm test -- --watchAll",
    "knex": "knex --knexfile server/db/knexfile.js",
    "db:delete": "rm -f server/db/dev.sqlite3 ",
    "db:rollback": "npm run knex migrate:rollback",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "db:reset": "run-s db:delete db:rollback db:rollback db:migrate db:seed",
    "postinstall": "run-s db:reset build",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ]
  },
  "dependencies": {
    "@mapbox/mapbox-gl-geocoder": "^4.5.1",
    "aos": "^2.3.4",
    "authenticare": "^0.4.6",
    "axios": "^0.21.1",
    "cloudinary": "^1.21.0",
    "cloudinary-core": "^2.8.2",
    "cloudinary-react": "^1.5.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.21.1",
    "npm-run-all": "^4.1.5",
    "oaf-react-router": "^2.1.1",
    "pg": "^8.0.3",
    "react-map-gl": "^5.2.5",
    "react-paginate": "^6.3.2",
    "react-slick": "^0.25.2",
    "slick-carousel": "^1.8.1",
    "sqlite3": "^4.2.0",
    "superagent": "^5.2.2",
    "sweetalert2": "^9.10.13",
    "sweetalert2-react": "^0.8.3",
    "sweetalert2-react-content": "^3.0.1",
    "webpack": "^4.43.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.9.4",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.2.6",
    "babel-loader": "^8.1.0",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^29.6.1",
    "lodash": "^4.17.19",
    "nodemon": "^2.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^16.13.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "supertest": "^4.0.2",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
